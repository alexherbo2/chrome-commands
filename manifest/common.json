{
  "manifest_version": 2,
  "name": "Commands",
  "description": "WebExtension API to perform browser actions",
  "version": "0.1.0",
  "permissions": [
    "downloads",
    "sessions",
    "notifications"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "packages/tools.png",
    "48": "packages/tools.png",
    "128": "packages/tools.png"
  }
}
